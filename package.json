{
  "name": "utility-style",
  "version": "1.0.0",
  "description": "使用tailwind.css自訂樣式設定",
  "scripts": {
    "cli-build": "set NODE_ENV=production&&npx tailwindcss-cli@latest build -o ./dist/tailwind.css",
    "postcss-build": "set NODE_ENV=production&&postcss ./entry.css -o ./dist/theme.min.css",
    "release": "standard-version --header=\"# Changelog \"",
    "release-first": "npm run release -- --first-release",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "exinfinite chen",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^10.2.4",
    "postcss": "^8.2.4",
    "tailwindcss": "^2.0.2",
    "cssnano": "^4.1.10",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0"
  }
}
